{
    "manifest_version": 2,
    "name": "MyApp",
    "description": "Description of the app",
    "version": "0.1.0",
    "app": {
        "background": {
            "scripts": [
                "background.js"
            ]
        }
    },
    "icons": {
        "128": "icon.png"
    },
    "permissions": [
        "clipboardWrite",
        "clipboardRead",
        "unlimitedStorage",
        "app.runtime",
        "app.window",
        "fileSystem",
        "socket",
        "geolocation",
        "usb",
        "storage",
        "system.display",
        "system.storage",
        "alarms",
        "pushMessaging",
        "notifications",
        "idle",
        "contextMenus"
    ],
    "offline_enabled": false,
    "optional_permissions": [],
    "externally_connectable": {
        "ids": [],
        "matches": [],
        "accepts_tls_channel_id": false
    }
}